{
  "Logging": {
    "LogLevel": {
      "Default": "Information", // Логи Information и выше
      "Microsoft.AspNetCore": "Warning", // Меньше шума от фреймворка
      "SearchService.Controllers.SearchController": "Debug", // Включаем Debug для контроллера
      "SearchService.Services.SearcherService": "Debug", // Включаем Debug для сервиса
      "SearchService.Services.IndexService": "Information", // Information для индексации
      "SearchService.Services.TextExtractorService": "Information" // Information для экстрактора
    }
  },
  "AllowedHosts": "*" // Или другие настройки
}